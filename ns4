BEGIN {
#include<stdio.h>
time=0
pack=0
}
{
if($1=="r"&&$3=="0"&&$4=="1")
{
pack=pack+$6
time=$2
}
}
END {
printf("Throughput %fKbps\n",(pack)/(time-2.5)*(8/1000));
}
